        "liste=[]\n",
        "\n",
        "def flat(x):\n",
        "\n",
        "  for eleman in x:\n",
        "\n",
        "    if type(eleman) == list:\n",
        "      flat(eleman)\n",
        "    else:\n",
        "      liste.append(eleman)\n",
        "  return liste\n",
        "\n",
        "\n"
      ],
