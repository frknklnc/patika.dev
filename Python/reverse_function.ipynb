        "def reverse_list(x):\n",
        "  x.reverse()\n",
        "  for i in x:\n",
        "      \n",
        "     if type(i) == list:\n",
        "    \n",
        "        i.reverse()\n",
        "     \n",
        "  return x\n"
      ],
